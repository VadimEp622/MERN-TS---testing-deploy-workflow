{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/src/index.js",
    "build": "swc src --out-dir dist",
    "build:ci": "tsc && npm run build",
    "build:watch": "swc src -w --out-dir dist",
    "nodemon:watch": "nodemon --delay 2.5 --watch \"dist/**/*\" -e js dist/src/index.js",
    "dev": "npm run build:ci && dotenvx run -f .env.dev.local -- concurrently \"npm run build:watch\" \"npm run nodemon:watch\"",
    "dev:live": "npm run build:ci && dotenvx run -f .env.dev.live -- concurrently \"npm run build:watch\" \"npm run nodemon:watch\"",
    "prod": "npm run build:ci && dotenvx run -f .env.prod.local -- concurrently \"npm run build:watch\" \"npm run nodemon:watch\"",
    "prod:live": "npm run build:ci && dotenvx run -f .env.prod.live -- concurrently \"npm run build:watch\" \"npm run nodemon:watch\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.31.3",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@swc/helpers": "^0.5.15",
    "@swc/plugin-transform-imports": "^6.0.2",
    "@tsconfig/node22": "^22.0.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.10.2",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "express": "^4.21.2",
    "mongoose": "^8.8.4",
    "node-cache": "^5.1.2",
    "nodemon": "^3.1.7",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.7"
  }
}
